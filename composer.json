{
    "name": "tusk/tusk-redis-mq-bundle",
    "type": "symfony-bundle",
    "description": "Symfony 2 Bundle for the tusk-redis-mq component",
    "keywords": ["redis", "message", "queue", "predis", "symfony"],
    "homepage": "http://github.com/tusk/TuskRedisMqBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernard van Niekerk",
            "email": "frizzy@paperjaw.com"
        }
    ],
    "require-dev": {
        "predis/predis": "0.7.*"
    },
    "require": {
        "php": ">=5.3.2",
        "tusk-tusk-redis-mq": "dev-master",
        "symfony/framework-bundle": "2.1.*",
        "symfony/yaml": "2.1.*"
    },
    "suggest": {
        "predis/predis": "If you want to use predis (currently only v0.7.x is supported).",
    },
    "autoload": {
        "psr-0": {"Tusk": ""}
    }
}